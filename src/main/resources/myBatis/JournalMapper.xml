<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.app.mapper.JournalMapper">
	<select id="journalList"
		resultType="com.example.app.domain.JournalClass">
		SELECT * FROM journal;
	</select>
	<select id="getAJournal" resultType="com.example.app.domain.JournalClass">
		SELECT * FROM journal
		WHERE journal_id = 1;
	</select>
</mapper>
